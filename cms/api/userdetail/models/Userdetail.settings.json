{
  "connection": "default",
  "collectionName": "userdetails",
  "info": {
    "name": "userdetail",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "user": {
      "model": "user",
      "via": "details",
      "plugin": "users-permissions"
    },
    "bio": {
      "type": "text"
    },
    "linkedin": {
      "type": "string"
    },
    "facebook": {
      "type": "string"
    },
    "twitter": {
      "type": "string"
    },
    "github": {
      "default": "disabled",
      "type": "string"
    },
    "www": {
      "default": "disabled",
      "type": "string"
    }
  }
}